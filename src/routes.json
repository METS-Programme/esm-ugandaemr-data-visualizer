{
  "$schema": "https://json.openmrs.org/routes.schema.json",
  "backendDependencies": {
    "fhir2": "^1.2.0",
    "webservices.rest": "^2.2.0"
  },
  "extensions": [
    {
      "name": "data-visualizer-dashboard-link",
      "component": "dataVisualizerDashboardLink",
      "slot": "homepage-dashboard-slot",
      "meta": {
        "name": "data-visualizer",
        "slot": "data-visualizer-dashboard-slot",
        "title": "Data visualizer"
      }
    },
    {
      "component": "root",
      "name": "data-visualizer-dashboard",
      "slot": "data-visualizer-dashboard-slot"
    },
    {
      "name": "user-dashboard-link",
      "component": "userDashboardLink",
      "slot": "homepage-dashboard-slot",
      "meta": {
        "name": "user-dashboard",
        "slot": "user-dashboard-slot",
        "title": "User Dashboard"
      }
    },
    {
      "component": "root",
      "name": "user-dashboard",
      "slot": "user-dashboard-slot"
    }
  ]
}
